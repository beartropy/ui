{
    "name": "beartropy/ui",
    "description": "UI components for Laravel/Livewire",
    "version": "0.7.22",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Beartropy\\Ui\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=8.0",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "tightenco/ziggy": "^2.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Beartropy\\Ui\\BeartropyUiServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.2",
        "pestphp/pest-plugin-livewire": "^3.0",
        "orchestra/testbench": "^10.4",
        "livewire/livewire": "^3.6"
    }
}
